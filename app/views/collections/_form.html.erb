<%= form_with model: collection, local: true do |form| %>
    <div>
        <%= form.label :title %>
        <%= form.text_field :title, value: collection.title %>
    </div>
    <div>
        <%= form.label :description %>
        <%= form.text_area :description, value: collection.description %>
    </div>
    <div>
        <%= form.submit %>
    </div>
<% end %>