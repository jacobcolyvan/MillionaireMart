<h1>All Designs</h1>

<%= link_to "Add Design", new_des_path%>

<br>
<ul>
  <% for design in @designs %>
    <li>
      <%= link_to "#{design.title}", design_path(design.id) %>
      <%= image_tag collection.picture if collection.picture.attached? %>
    </li>
  <% end %>
</ul>

